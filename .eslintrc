{
    "extends": [
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier",
        "react",
        "jsx-a11y"
    ],
    "parser": "babel-eslint",
    "rules": {
        "semi": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "prettier/prettier": [
            "error"
        ],
        "react/prop-types": 0,
        "import/no-unresolved": "off",
        "jsx-a11y/no-onchange": "off"
    },
    "globals": {
        "fetch": false
    }
}